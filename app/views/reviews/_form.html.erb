<%= form_with model: [@room, @room.reviews.new], local: true do |f|%>

    <div id="stars"></div>
    <%= f.text_field :comment %>
    <%= f.hidden_field :room_id, value: @room.id %>

    <div class="actions">
        <%= f.submit %>
    </div>
<% end %>

<script>
    $('#stars').raty({
        size: 36,
        path: '/assets/',
        scoreName: 'review[star]',
        score: 1,
       

    });
</script>